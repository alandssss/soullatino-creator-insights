{
  "id": "/",
  "name": "Soullatino Analytics - Dashboard de Creadores TikTok",
  "short_name": "Soullatino",
  "description": "Plataforma de analytics para monitorear y optimizar el rendimiento de creadores de contenido en TikTok",
  "start_url": "/",
  "scope": "/",
  "display": "standalone",
  "display_override": ["tabbed", "window-controls-overlay", "standalone"],
  "background_color": "#0a0f1e",
  "theme_color": "#d4ff6a",
  "orientation": "portrait-primary",
  "lang": "es-MX",
  "dir": "ltr",
  "categories": ["business", "productivity", "analytics"],
  "prefer_related_applications": false,
  "related_applications": [
    {
      "platform": "play",
      "url": "https://play.google.com/store/apps/details?id=app.lovable.f8b38fe6e36c4ad5b1203b74ef284578",
      "id": "app.lovable.f8b38fe6e36c4ad5b1203b74ef284578"
    },
    {
      "platform": "itunes",
      "url": "https://apps.apple.com/app/soullatino-analytics/id123456789"
    }
  ],
  "iarc_rating_id": "e84b072d-71b3-4d3e-86ae-31a8ce4e53b7",
  "scope_extensions": [
    {
      "origin": "*.soullatino.com"
    }
  ],
  "file_handlers": [
    {
      "action": "/dashboard/upload",
      "accept": {
        "application/vnd.openxmlformats-officedocument.spreadsheetml.sheet": [".xlsx"],
        "application/vnd.ms-excel": [".xls"],
        "text/csv": [".csv"]
      },
      "icons": [
        {
          "src": "/logo.png",
          "sizes": "512x512"
        }
      ],
      "launch_type": "single-client"
    }
  ],
  "note_taking": {
    "new_note_url": "/dashboard/pending"
  },
  "widgets": [
    {
      "name": "Feedback Pendiente",
      "short_name": "Pendiente",
      "description": "Vista rápida del feedback pendiente",
      "tag": "feedback-widget",
      "template": "feedback-pending",
      "ms_ac_template": "/widgets/feedback-pending.json",
      "data": "/api/widgets/feedback-pending",
      "type": "application/json",
      "screenshots": [
        {
          "src": "/logo.png",
          "sizes": "512x512",
          "label": "Widget de feedback pendiente"
        }
      ],
      "icons": [
        {
          "src": "/logo.png",
          "sizes": "512x512"
        }
      ],
      "auth": true,
      "update": 900
    }
  ],
  "icons": [
    {
      "src": "/logo.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "any maskable"
    }
  ],
  "screenshots": [
    {
      "src": "/logo.png",
      "sizes": "512x512",
      "type": "image/png",
      "form_factor": "wide",
      "label": "Dashboard de Analytics de Creadores TikTok"
    },
    {
      "src": "/logo.png",
      "sizes": "512x512",
      "type": "image/png",
      "form_factor": "narrow",
      "label": "Vista móvil del dashboard"
    }
  ],
  "shortcuts": [
    {
      "name": "Feedback Pendiente",
      "short_name": "Pendiente",
      "description": "Ver feedback pendiente de creadores",
      "url": "/dashboard/pending",
      "icons": [{ "src": "/logo.png", "sizes": "512x512" }]
    },
    {
      "name": "Historial",
      "short_name": "Historial",
      "description": "Ver historial de feedback",
      "url": "/dashboard/history",
      "icons": [{ "src": "/logo.png", "sizes": "512x512" }]
    },
    {
      "name": "Lista de Creadores",
      "short_name": "Creadores",
      "description": "Ver todos los creadores",
      "url": "/dashboard/creators",
      "icons": [{ "src": "/logo.png", "sizes": "512x512" }]
    }
  ],
  "share_target": {
    "action": "/dashboard/creators",
    "method": "GET",
    "enctype": "application/x-www-form-urlencoded",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url"
    }
  },
  "launch_handler": {
    "client_mode": ["navigate-existing", "auto"]
  },
  "edge_side_panel": {
    "preferred_width": 400
  }
}
